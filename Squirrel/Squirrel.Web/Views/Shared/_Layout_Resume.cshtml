<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="keywords" content="رزومه,مهارت,تجربه,تجربیات,بهنام,ضیغمی,بهنام ضیغمی,cv,resume,skill,experience,behnam,zeighami,behnam zeighami,it,web application,itman" />
    <meta name="description" content="تجربیات و مهارتهای بهنام ضیغمی، رزومه کاری" />
    <link rel="shortcut icon" href="~/Content/themes/vone/images/favicon.png" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Styles.Render("~/Content/vone")
    @RenderSection("scripts", required: false)
    <script type="text/javascript">
        $(function () {
            $(".header-container .header-search").css("display", "none");
        });
    </script>
</head>
<body>
    <div class="container">
        <div class="header-container" id="header_fix">
            @Html.Partial("_Header")
        </div>
        <div class="bubble-menu hidden-lg hidden-md">
            @Html.Partial("_BubbleMenu")
        </div>
        <div class="row">
            @RenderBody()
        </div>
        <div class="footer-container">
            @Html.Partial("_Footer")
        </div>
    </div>
</body>
</html>
